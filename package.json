{
  "name": "blog",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "MonkeyBinBin <thisisbinbin@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxt start",
    "generate": "node scripts/generateRoutes.mjs && nuxi generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "npx stylelint '**/*.{css,scss,sass,vue}'",
    "lint": "npm run lint:js && npm run lint:css",
    "predeploy": "npm run generate",
    "deploy": "npx del-cli -f 'public/sitemap.xml' 'public/sitemap.xml.gz' && gh-pages -d dist -b master",
    "deploy:ci": "npx del-cli -f 'public/sitemap.xml' 'public/sitemap.xml.gz' && gh-pages -d dist -b master -r https://$GITHUB_TOKEN@github.com/MonkeyBinBin/monkeybinbin.github.io.git",
    "prebuild": "node ./scripts/generateRoutes.mjs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@nuxtjs/google-tag-manager": "^2.3.2",
    "@vueuse/motion": "^3.0.3",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "contentful": "^11.7.3",
    "glob-all": "^3.3.1",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "marked": "^16.0.0",
    "moment": "^2.30.1",
    "motion": "^12.19.2",
    "nuxt": "^3.17.5",
    "sass": "^1.77.6",
    "sass-loader": "^16.0.5",
    "vue-disqus": "^5.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-standard": "^17.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.33.0",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.38",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "purgecss-webpack-plugin": "^7.0.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^4.7.0",
    "vue-eslint-parser": "^9.4.3"
  }
